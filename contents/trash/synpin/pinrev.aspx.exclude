    <script type="text/javascript">
        function CheckIfCheckBoxListIsChecked(source, args) {
            var chkList = document.getElementById('<%= cbx_47.ClientID%>');
            var chk = chkList.getElementsByTagName("input");
            for (var i = 0; i < chk.length; i++) {
                if (chk[i].checked) {
                    args.IsValid = true;
                    return;
                }
            }
            args.IsValid = false;
        }
    </script>

